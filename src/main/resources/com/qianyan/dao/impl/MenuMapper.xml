<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianyan.dao.MenuMapper">
	
	<insert id="insert" parameterType="com.qianyan.entity.Menu">
		insert into t_menu(name,url)
		values(#{name},#{url,jdbcType=VARCHAR})
	</insert>

   <select id="getAll" resultType="com.qianyan.entity.Menu">
       select * from t_menu;
   </select>
	
</mapper>
	